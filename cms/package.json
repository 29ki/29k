{
  "name": "29k-cms",
  "version": "2.0.0",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "build:content": "yarn --cwd \"../content\" build",
    "build": "yarn build:content && rm -rf dist && vite build",
    "build:types": "yarn ts-node ./generateTypes.ts",
    "dev": "yarn build:content && vite & PORT=1337 GIT_REPO_DIRECTORY='./../' yarn run decap-server",
    "start": "yarn build:content && vite",
    "test:jest": "jest",
    "test:lint": "eslint --ext .js,.jsx,.ts,.tsx",
    "test:ts": "tsc --noEmit",
    "test": "yarn build:content && yarn test:ts && yarn test:lint && yarn test:jest"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "decap-cms-app": "3.6.2",
    "decap-cms-media-library-cloudinary": "^3.0.3",
    "i18next": "^24.2.2",
    "react-native-safe-area-context": "^5.2.0",
    "react-native-svg": "15.4.0",
    "react-native-web": "^0.19.13",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-native-web": "^0.19.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.3.4",
    "decap-server": "^3.1.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "netlify-ts": "^2.0.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^6.1.1"
  }
}
